<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="generated.app.controller.page_2" xmlns:m="sap.m" xmlns:layout="sap.ui.layout">
	<m:Page showHeader="true" title="Customer Trading History" showFooter="true" showNavButton="true" navButtonPress="_onPageNavButtonPress">
		<m:content>
			<m:Panel height="auto" width="100%" headerText="Trading" expandable="false" expanded="true" expandAnimation="true" backgroundDesign="Solid">
				<m:content>
					<m:ScrollContainer horizontal="true" vertical="true" width="100%" height="auto" visible="true">
						<m:content>
							<m:Table width="100%" noDataText="No data" mode="None" showSeparators="All" growing="true" growingThreshold="20" growingScrollToLoad="true">
								<m:infoToolbar>
									<m:Toolbar width="100%" height="auto" design="Auto" visible="false" enabled="true">
										<m:content>
											<m:Label text="Label" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
										</m:content>
									</m:Toolbar>
								</m:infoToolbar>
								<m:headerToolbar/>
								<m:columns>
									<m:Column id="Description" width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<m:header>
											<m:Text text="Period Description" width="auto" maxLines="1" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
										</m:header>
										<m:footer/>
									</m:Column>
									<m:Column id="ClrDocNo" width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<m:header>
											<m:Text text="Clrng Doc." width="auto" maxLines="1" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
										</m:header>
										<m:footer/>
									</m:Column>
									<m:Column id="Payments" width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<m:header>
											<m:Text text="Sales Amount" width="auto" maxLines="1" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
										</m:header>
										<m:footer/>
									</m:Column>
									<m:Column id="Payments" width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<m:header>
											<m:Text text="Payment Amount" width="auto" maxLines="1" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
										</m:header>
										<m:footer/>
									</m:Column>
									<m:Column id="CalcDays" width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<m:header>
											<m:Text text="Days" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
										</m:header>
										<m:footer/>
									</m:Column>
								</m:columns>
								<m:items>
									<m:ColumnListItem type="Active">
										<cells> 
											<layout:HorizontalLayout>
												<Text text="{Description}"/>
											</layout:HorizontalLayout>
											<layout:HorizontalLayout>
												<Text text="{clrDocNo}"/>
											</layout:HorizontalLayout>
											<layout:HorizontalLayout>
												<Text text="{Payments}"/>
											</layout:HorizontalLayout>
											<layout:HorizontalLayout>
												<Text text="{Payments}"/>
											</layout:HorizontalLayout>
											<layout:HorizontalLayout>
												<Text text="{CalcDays}"/>
											</layout:HorizontalLayout>
										</cells>
									</m:ColumnListItem>
								</m:items>
							</m:Table>
						</m:content>
					</m:ScrollContainer>
					<m:Panel height="auto" width="100%" headerText="Payments &amp; Dates" expandable="true" expanded="true" expandAnimation="true"
						backgroundDesign="Solid">
						<m:content>
							<m:ScrollContainer horizontal="true" vertical="true" width="100%" height="auto" visible="true">
								<m:content>
									<m:Table width="100%" noDataText="No data" mode="None" showSeparators="All" growing="true" growingThreshold="20" growingScrollToLoad="true">
										<m:infoToolbar>
											<m:Toolbar width="100%" height="auto" design="Auto" visible="false" enabled="true">
												<m:content>
													<m:Label text="Label" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
												</m:content>
											</m:Toolbar>
										</m:infoToolbar>
										<m:headerToolbar/>
										<m:columns>
											<m:Column id= "Description" width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
												<m:header>
													<m:Text text="Period Description" width="auto" maxLines="1" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
												</m:header>
												<m:footer/>
											</m:Column>
											<m:Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
												<m:header>
													<m:Text id= "ClrDocNo" text="Clrng doc." width="auto" maxLines="1" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
												</m:header>
												<m:footer/>
											</m:Column>
											<m:Column id="Payments" width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
												<m:header>
													<m:Text text="Payments Amount" width="auto" maxLines="1" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
												</m:header>
												<m:footer/>
											</m:Column>
											<m:Column id="ClrDocNo" width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
												<m:header>
													<m:Text text="Clearing Date" width="auto" maxLines="1" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
												</m:header>
												<m:footer/>
											</m:Column>
											<m:Column id="Faedt" width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
												<m:header>
													<m:Text text="Net Due Date" width="auto" maxLines="1" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
												</m:header>
												<m:footer/>
											</m:Column>
											<m:Column id="days" width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
												<m:header>
													<m:Text text="Days" width="auto" maxLines="1" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
												</m:header>
												<m:footer/>
											</m:Column>
										</m:columns>
										<m:items>
											<m:ColumnListItem type="Active">
													<layout:HorizontalLayout>
														<Text text="{Description}"/>
													</layout:HorizontalLayout>
													<layout:HorizontalLayout>
														<Text text="{ClrDocNo}"/>
													</layout:HorizontalLayout>
													<layout:HorizontalLayout>
														<Text text="{Payments}"/>
													</layout:HorizontalLayout>
													<layout:HorizontalLayout>
															<Text text="{ClearDate}"/>
													</layout:HorizontalLayout>
													<layout:HorizontalLayout>
															<Text text="{Faedt}"/>
													</layout:HorizontalLayout>
													<layout:HorizontalLayout>
															<Text text="{CalcDays}"/>
													</layout:HorizontalLayout>
											</m:ColumnListItem>
										</m:items>
									</m:Table>
								</m:content>
							</m:ScrollContainer>
						</m:content>
					</m:Panel>
					<m:Panel height="auto" width="100%" headerText="Discount" expandable="true" expanded="true" expandAnimation="true" backgroundDesign="Solid">
						<m:content>
							<m:ScrollContainer horizontal="true" vertical="true" width="100%" height="auto" visible="true">
								<m:content>
									<m:Table width="100%" noDataText="No data" mode="None" showSeparators="All" growing="true" growingThreshold="20" growingScrollToLoad="true">
										<m:infoToolbar>
											<m:Toolbar width="100%" height="auto" design="Auto" visible="false" enabled="true">
												<m:content>
													<m:Label text="Label" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
												</m:content>
											</m:Toolbar>
										</m:infoToolbar>
										<m:headerToolbar/>
										<m:columns>
											<m:Column id="Description" width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
												<m:header>
													<m:Text text="Period Description" width="auto" maxLines="1" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
												</m:header>
												<m:footer/>
											</m:Column>
											<m:Column id="ClrDocNo" width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
												<m:header>
													<m:Text text="Clrng doc." width="auto" maxLines="1" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
												</m:header>
												<m:footer/>
											</m:Column>
											<m:Column id="DiscountPercentage" width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
												<m:header>
													<m:Text text="Discount Percentage" width="auto" maxLines="1" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
												</m:header>
												<m:footer/>
											</m:Column>
											<m:Column id="Discount" width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
												<m:header>
													<m:Text text="Sales Discount" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
												</m:header>
												<m:footer/>
											</m:Column>
										</m:columns>
										<m:items>
											<m:ColumnListItem type="Active">
													<layout:HorizontalLayout>
														<Text text="{Description}"/>
													</layout:HorizontalLayout>
													<layout:HorizontalLayout>
														<Text text="{ClrDocNo}"/>
													</layout:HorizontalLayout>
													<layout:HorizontalLayout>
														<Text text="{DiscountPercentage}"/>
													</layout:HorizontalLayout>
													<layout:HorizontalLayout>
														<Text text="{Discount}"/>
													</layout:HorizontalLayout>
											</m:ColumnListItem>
										</m:items>
									</m:Table>
								</m:content>
							</m:ScrollContainer>
						</m:content>
					</m:Panel>
				</m:content>
			</m:Panel>
		</m:content>
		<m:footer>
			<m:Bar design="Auto">
				<m:contentLeft>
					<m:Button text="Back" type="Back" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"
						press="_onButtonPress"/>
				</m:contentLeft>
				<m:contentMiddle/>
				<m:contentRight/>
			</m:Bar>
		</m:footer>
		<m:headerContent/>
		<m:subHeader/>
		<m:customHeader/>
	</m:Page>
</mvc:View>